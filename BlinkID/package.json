{
  "name": "blinkid-react-native-ts",
  "version": "6.1.1",
  "description": "A small and powerful ID card scanning library. Powered by Microblink (www.microblink.com).",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "index",
  "source": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlinkID/blinkid-react-native.git"
  },
  "keywords": [
    "scan",
    "id",
    "scanner",
    "identity",
    "ID",
    "passport",
    "licence",
    "sdk",
    "ocr",
    "USDL",
    "PDF417",
    "MRZ",
    "MRTD",
    "barcode",
    "DL",
    "card",
    "document",
    "microblink",
    "react-native",
    "blinkid",
    "ID",
    "ios",
    "android"
  ],
  "author": "Microblink",
  "license": "MIT",
  "homepage": "https://github.com/BlinkID/blinkid-react-native",
  "bugs": {
    "url": "https://github.com/BlinkID/blinkid-react-native/issues"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it"
  },
  "devDependencies": {
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-builder-bob": "^0.20.4",
    "release-it": "^15.0.0",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "rnpm": {
    "android": {
      "sourceDir": "./src/android"
    },
    "ios": {
      "sourceDir": "./src/ios"
    }
  },
  "files": [
    "./",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "./",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
